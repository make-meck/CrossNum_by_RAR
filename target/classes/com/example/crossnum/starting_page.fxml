            <font>
                <Font name="Arial Bold" size="20.0" />
            </font>
        </Button>
        <Button fx:id="medium_button" layoutX="241.0" layoutY="237.0" mnemonicParsing="false" prefHeight="57.0" prefWidth="204.0" styleClass="medium_button" text="MEDIUM" textFill="#354b34" visible="false">

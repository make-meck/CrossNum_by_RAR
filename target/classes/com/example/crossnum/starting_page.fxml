<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.crossnum.StartingPage">
    <children>
        <ImageView fitHeight="571.0" fitWidth="712.0" layoutX="-27.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@startingbg.png" />
            </image>
        </ImageView>
        <Text fill="#354b34" layoutX="226.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="CROSSNUM" textAlignment="CENTER">
            <font>
                <Font name="Arial Bold" size="40.0" />
            </font>
        </Text>
        <Button fx:id="play_button" alignment="CENTER" layoutX="254.0" layoutY="242.0" mnemonicParsing="false" onAction="#playClick" prefHeight="47.0" prefWidth="178.0" styleClass="play_button" text="PLAY" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Arial Bold" size="25.0" />
            </font>
        </Button>
        <Button fx:id="help_button" layoutX="442.0" layoutY="242.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="43.0" styleClass="help_button" />
        <ImageView fitHeight="38.0" fitWidth="32.0" layoutX="448.0" layoutY="250.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@helpbutton.png" />
            </image>
        </ImageView>
        <Rectangle fx:id="bg_shadow" height="401.0" opacity="0.55" stroke="BLACK" strokeType="INSIDE" styleClass="bg_shadow" visible="false" width="685.0" />
        <Rectangle fx:id="level_container" arcHeight="15.0" arcWidth="15.0" fill="WHITE" height="267.0" layoutX="179.0" layoutY="134.0" stroke="BLACK" strokeType="INSIDE" styleClass="level_container" visible="false" width="328.0" />
